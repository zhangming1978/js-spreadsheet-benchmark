{
  "name": "js-spreadsheet-benchmark",
  "private": false,
  "version": "1.0.0",
  "description": "An open-source performance benchmark tool for comparing mainstream JavaScript spreadsheet libraries.",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "smoke-test": "playwright test tests/e2e/smoke-test.spec.ts",
    "smoke-test:headed": "playwright test tests/e2e/smoke-test.spec.ts --headed"
  },
  "dependencies": {
    "@grapecity-software/spread-sheets": "^19.0.1",
    "@grapecity-software/spread-sheets-react": "^19.0.1",
    "@handsontable/react-wrapper": "^16.2.0",
    "@univerjs/core": "^0.15.5",
    "@univerjs/design": "^0.15.5",
    "@univerjs/engine-formula": "^0.15.5",
    "@univerjs/engine-render": "^0.15.5",
    "@univerjs/presets": "^0.15.5",
    "@univerjs/sheets": "^0.15.5",
    "@univerjs/sheets-formula": "^0.15.5",
    "@univerjs/sheets-ui": "^0.15.5",
    "@univerjs/ui": "^0.15.5",
    "antd": "^5.22.6",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "handsontable": "^16.2.0",
    "html2canvas": "^1.4.1",
    "jquery": "^4.0.0",
    "jquery-mousewheel": "^3.2.2",
    "jspdf": "^4.1.0",
    "jspreadsheet-ce": "^5.0.4",
    "lodash-es": "^4.17.21",
    "luckysheet": "^2.1.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "x-data-spreadsheet": "^1.1.9",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/jquery": "^3.5.33",
    "@types/jquery-mousewheel": "^3.1.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "less": "^4.5.1",
    "playwright": "^1.58.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
